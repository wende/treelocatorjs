{
  "name": "@treelocator/runtime",
  "version": "0.1.0",
  "description": "TreeLocatorJS runtime for component ancestry tracking. Alt+click any element to copy its component tree to clipboard. Exposes window.__locatorjs__ API for browser automation (Playwright, Puppeteer, Selenium, Cypress).",
  "keywords": [
    "locator",
    "locatorjs",
    "component",
    "ancestry",
    "debugging",
    "devtools",
    "react",
    "vue",
    "svelte",
    "playwright",
    "puppeteer",
    "selenium",
    "cypress",
    "automation",
    "testing",
    "e2e"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "vitest run",
    "test:dev": "vitest watch",
    "dev": "concurrently pnpm:dev:*",
    "dev:babel": "babel src --watch --out-dir dist --extensions .js,.jsx,.ts,.tsx",
    "//dev:babel-cjs": "babel src --watch --out-dir dist --extensions .js,.jsx,.ts,.tsx --out-file-extension .cjs --plugins @babel/plugin-transform-modules-commonjs",
    "dev:ts": "tsc --watch --declaration --emitDeclarationOnly --noEmit false --outDir dist --preserveWatchOutput",
    "dev:tailwind": "tailwindcss -i ./src/main.css -o ./dist/output.css --watch",
    "dev:wrapCSS": "WATCH=true node ./scripts/wrapCSS.js",
    "dev:wrapImage": "WATCH=true node ./scripts/wrapImage.js",
    "build": "concurrently pnpm:build:*",
    "build:babel": "babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx",
    "//build:babel-cjs": "babel src --out-dir dist --extensions .js,.jsx,.ts,.tsx --out-file-extension .cjs --plugins @babel/plugin-transform-modules-commonjs",
    "build:ts": "tsc --declaration --emitDeclarationOnly --noEmit false --outDir dist",
    "build:tailwind": "tailwindcss -i ./src/main.css -o ./dist/output.css",
    "build:wrapImage": "node ./scripts/wrapImage.js",
    "ts": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "@locator/shared": "^0.5.0",
    "solid-js": "^1.9.2",
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@treelocator/dev-config": "^0.1.0",
    "@types/jsdom": "^21.1.7",
    "babel-preset-solid": "^1.9.2",
    "concurrently": "^9.1.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "vue": "^3.5.12"
  },
  "author": "Krzysztof Wende",
  "repository": {
    "type": "git",
    "url": "https://github.com/wende/treelocatorjs",
    "directory": "packages/runtime"
  },
  "license": "MIT",
  "gitHead": "53bca74332ce143e672983b2b0667eee5dd0730f"
}
